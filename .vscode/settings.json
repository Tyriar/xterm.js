{
  "files.associations": {
    ".eslintrc.json.typings": "jsonc"
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "mochaExplorer.envPath": ".mocha.env",
  "mochaExplorer.files": [
    "out-tsc/**/*.test.js",
    "addons/**/out-tsc/*.test.js"
  ],
  "mochaExplorer.watch": [
    "out-tsc/**/*.js",
    "addons/**/out-tsc/*.js"
  ]
}
